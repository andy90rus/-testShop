{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "noResolve": false,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}
